<div id="notice" class="box generalbox modal modal-dialog modal-in-page show">
  <div id="modal-content" class="box modal-content">
    <div id="modal-header" class="box modal-header">
      <h4>{{#str}}confirm_heading, block_anderspink{{/str}}</h4>
    </div>
    <div id="modal-body" class="box modal-body">
      {{{duplicate_msg}}}
    </div>
    <div id="modal-footer" class="box modal-footer">
      <div class="buttons">
        <div class="singlebutton">
          <form method="post" action="{{post_url}}">
            <div>
              <input type="submit" value="Confirm" class="form-submit btn-primary">
              <input type="hidden" name="id" value="{{id}}">
              <input type="hidden" name="course" value="{{course}}">
              <input type="hidden" name="instance" value="{{instance}}">
              <input type="hidden" name="dashboardid" value="{{dashboardid}}">
              <input type="hidden" name="sesskey" value="{{sesskey}}">
              <input type="hidden" name="config_name" value="{{name}}">
              <input type="hidden" name="config_source" value="{{config_source}}">
              <input type="hidden" name="config_team" value="{{config_team}}">
              <input type="hidden" name="config_briefing" value="{{config_briefing}}">
              <input type="hidden" name="config_board" value="{{config_board}}">
              <input type="hidden" name="config_briefing_time" value="{{config_briefing_time}}">
              {{#config_audience}}
                <input type="hidden" name="config_audience[]" value="{{.}}">
              {{/config_audience}}
              <input type="hidden" name="confirm" value="true">
              <input type="hidden" name="return" value="{{return}}">
            </div>
          </form>
        </div>
        <div class="singlebutton">
          <form method="get" action="/totara/dashboard/index.php">
            <div>
              <input type="submit" value="Cancel">
              <input type="hidden" name="bui_editid" value="{{instance}}">
              <input type="hidden" name="id" value="{{course}}">
              <input type="hidden" name="dashboardid" value="{{dashboardid}}">
              <input type="hidden" name="sesskey" value="{{sesskey}}">
              <input type="hidden" name="return" value="{{return}}">
            </div>
          </form>
        </div>
      </div>
    </div>
  </div>
</div>